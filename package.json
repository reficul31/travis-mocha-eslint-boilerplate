{
  "name": "mocha-travis-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate code for testing with mocha and continuous integration with travis.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/*.test.js",
    "build": "babel --presets es2015 src/*.js -o build/main.bundle.js",
    "lint": "standard-own --verbose | snazzy",
    "fix": "standard-own --fix |snazzy"
  },
  "author": "Shivang Bharadwaj",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "mocha": "^3.2.0",
    "snazzy": "^6.0.0",
    "standard": "^9.0.2",
    "standard-own": "^1.0.2"
  },
  "standard-own": {
    "eslintConfig": {},
    "ignore": [
      "extension/"
    ]
  }
}
