{
  "name": "mocha-travis-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate code for testing with mocha and continuous integration with travis.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/*.test.js",
    "build": "babel --presets es2015 src/*.js -o build/main.bundle.js"
  },
  "author": "Shivang Bharadwaj",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "mocha": "^3.2.0"
  }
}
